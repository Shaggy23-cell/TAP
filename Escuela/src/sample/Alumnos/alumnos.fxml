<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="940.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Alumnos.Alumnos">
   <children>
      <VBox layoutX="-3.0" prefHeight="400.0" prefWidth="154.0" style="-fx-background-color: black;">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="158.0" style="-fx-background-color: black;" text="Nombre de  Alumno" textAlignment="CENTER" textFill="WHITE" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <TextField fx:id="txtName" alignment="CENTER" promptText="Nombres">
               <VBox.margin>
                  <Insets top="-2.0" />
               </VBox.margin>
            </TextField>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="158.0" style="-fx-background-color: black;" text="Apellidos " textAlignment="CENTER" textFill="WHITE" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="txtLastName" alignment="CENTER" promptText="Apellidos">
               <VBox.margin>
                  <Insets top="-2.0" />
               </VBox.margin>
            </TextField>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="158.0" style="-fx-background-color: black;" text="Telefono" textAlignment="CENTER" textFill="#eeeeee" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="txtPhone" alignment="CENTER" promptText="Telefono" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="158.0" style="-fx-background-color: black;" text="Carrera" textAlignment="CENTER" textFill="WHITE" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="txtCareer" alignment="CENTER" promptText="Carrera" />
            <Button fx:id="btnInsertar" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#insertar" prefHeight="40.0" prefWidth="83.0" style="-fx-background-color: #34495E; -fx-background-radius: 30;" text="Insertar" textAlignment="CENTER" textFill="WHITE" textOverrun="CLIP">
               <VBox.margin>
                  <Insets left="40.0" top="50.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold Italic" size="15.0" />
               </font>
            </Button>
            <Button fx:id="btnCancelar" alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="246.0" mnemonicParsing="false" onAction="#cancelar" prefHeight="40.0" prefWidth="83.0" style="-fx-background-color: #34495E; -fx-background-radius: 30;" text="Cancelar" textAlignment="CENTER" textFill="WHITE" textOverrun="CLIP" visible="false">
               <font>
                  <Font name="System Bold Italic" size="15.0" />
               </font>
               <VBox.margin>
                  <Insets left="40.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#regresar" prefHeight="27.0" prefWidth="54.0" style="-fx-background-color: #34495E; -fx-background-radius: 30;" text="←" textFill="WHITE">
               <font>
                  <Font name="Berlin Sans FB" size="15.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="6.0" left="3.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <TableView fx:id="table" layoutX="151.0" prefHeight="400.0" prefWidth="789.0" stylesheets="@../../estilos.css" />
   </children>
</AnchorPane>
